!!!
%html
  %head
    %title RSSS | DASHBOARD
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js'
    = javascript_include_tag 'js_class'
    = javascript_include_tag 'bluff'
    = javascript_include_tag 'excanvas'
    = javascript_include_tag 'application'
    = csrf_meta_tag
    %link{:rel=>"icon", :type=>"image/png", :href=>"/favicon.png"}
    = yield :head
  %body
    #head
      %img.icon{:src => current_user.profile_image_url}
        %h1 DASHBOARD
      %div.clear
        .description #{current_user.screen_name}(#{current_user.name})
        .description= current_user.description

    = render :partial=>'layouts/menu'
    = yield
    = render :partial=>'layouts/footer'
    = render :partial=>'layouts/analytics'
